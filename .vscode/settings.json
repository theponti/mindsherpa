{
  "[typescript]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "biomejs.biome"
  },
  "cSpell.ignoreWords": [
    "graphcache",
    "openai",
    "pydantic",
    "rneui",
    "supabase",
    "Mindsherpa",
    "Ionicons",
    "noto",
    "rneui",
    "supabase",
    "Mindsherpa",
    "Ionicons",
    "noto",
    "urql",
    "pressable",
    "autogrow"
  ],
  "cSpell.words": [
    "biomejs",
    "codegen",
    "dripsy",
    "meterings",
    "Millis",
    "netinfo",
    "ponti",
    "pontistudios",
    "reactotron",
    "tanstack",
    "xmark",
    "zeego"
  ],
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": true,
  "editor.defaultFormatter": "biomejs.biome",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "editor.codeActionsOnSave": {
    "source.sortImports": "always",
    "source.organizeImports": "always",
    "source.removeUnusedImports": "always"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
