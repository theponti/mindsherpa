{
  "name": "mindsherpa",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "NODE_ENV=development expo start --dev-client",
    "ios": "NODE_ENV=development expo run:ios",
    "android": "NODE_ENV=development expo run:android",
    "build:simulator:android": "NODE_ENV=development eas build --profile simulator --platform android",
    "build:simulator:ios": "NODE_ENV=development eas build --profile simulator --platform ios",
    "build:development:android": "NODE_ENV=development eas build --profile development --platform android",
    "build:development:ios": "NODE_ENV=development eas build --profile development --platform ios",
    "build:preview:android": "NODE_ENV=preview eas build --profile preview --platform android",
    "build:preview:ios": "NODE_ENV=preview eas build --profile preview --platform ios",
    "build:production:android": "NODE_ENV=production eas build --profile production --platform android",
    "build:production:ios": "NODE_ENV=production eas build --profile production --platform ios",
    "build:update:preview": "NODE_ENV=production eas update --platform ios --branch preview --clear-cache",
    "prebuild": "expo prebuild",
    "web": "expo start --web",
    "codegen": "openapi-typescript http://localhost:8002/openapi.json --output utils/api-types.ts"
  },
  "dependencies": {
